{
  "name": "react-voice-skill",
  "version": "0.0.1",
  "description": "A renderer and collection of components to write Alexa Skills in React",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "build": "babel src -d lib",
    "demo": "babel-node examples/hello-world"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamgruber/react-voice-skill.git"
  },
  "keywords": [
    "react",
    "alexa"
  ],
  "author": "Adam Gruber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamgruber/react-voice-skill/issues"
  },
  "homepage": "https://github.com/adamgruber/react-voice-skill#readme",
  "dependencies": {
    "alexa-sdk": "^1.0.25",
    "jest": "^22.4.3",
    "react": "^16.2.0",
    "react-reconciler": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "lerna": "^2.9.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/testRenderer.js"
    ]
  }
}
